        </dt>
        <dd class = "col-sm-7">
            @Html.DisplayFor(model => model.idAnimal)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="idVacina" />
        <input type="submit" value="Remover" class="btn btn-danger" /> 
        <a asp-action="Index" class="btn btn-primary">Voltar</a>
    </form>
